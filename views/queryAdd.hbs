<script type="text/javascript">

    var x =1;
    function addMobile(){
        var a = document.getElementById("mob-2");
        var b = document.getElementById("mob-3");
        if(x==1) {
            a.style.display = "block";
            x = x+1;
        }
        else if(x==2){
            b.style.display = "block";
            x = x+1;
        }
        else {
            alert("You can add maximum 3 mobile numbers.");
        }
    };

    function otherBox(){
        var othervalue = document.getElementById("otherselect").value;

        if(othervalue=="other"){
            document.getElementById('otherOcc').disabled = false;
            document.getElementById("otherocc").style.display = "block";
        }
        else {
            document.getElementById("otherocc").style.display = "none";
        }
    };
    function showModel(){

        if(document.getElementById("model").style.display == "none"){
            document.getElementById("model").style.display = "block";
        }
        else {
            document.getElementById("model").style.display = "none";
        }
    };

</script>
<script type="text/javascript">
    $(document).ready(function (){


        var today = new Date();
        var dd = today.getDate();

        var mm = today.getMonth()+1;
        var yyyy = today.getFullYear();
        if(dd<10)
        {
            dd='0'+dd;
        }

        if(mm<10)
        {
            mm='0'+mm;
        }
        today = yyyy+'-'+mm+'-'+dd;
        $('.det').attr("max", today);
        // $('.det').html('<input type="date" max="' + today + '" class="form-control" id="dob">');

    });

</script>
